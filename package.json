{
  "name": "shopify-metadata-migrator",
  "version": "1.0.3",
  "description": "A CLI tool for migrating Shopify metafield and metaobject definitions between stores",
  "main": "migrateShopifyMetafields.js",
  "bin": {
    "shopify-metadata-migrator": "migrateShopifyMetafields.js"
  },
  "scripts": {
    "start": "node migrateShopifyMetafields.js",
    "test": "node migrateShopifyMetafields.js --help",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "keywords": [
    "shopify",
    "metafields",
    "metaobjects",
    "metadata",
    "migration",
    "cli",
    "copy",
    "transfer",
    "development",
    "production"
  ],
  "author": "Zion Emond",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warriorachilles/shopify-metadata-migrator.git"
  },
  "bugs": {
    "url": "https://github.com/warriorachilles/shopify-metadata-migrator/issues"
  },
  "homepage": "https://github.com/warriorachilles/shopify-metadata-migrator#readme",
  "preferGlobal": true,
  "files": [
    "migrateShopifyMetafields.js",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "prettier": "^3.6.2"
  }
}
